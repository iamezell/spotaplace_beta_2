    <body>

      <div class="navbar-wrapper">
        <div class="container">

          <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="container">
              <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                  <span class="sr-only">Toggle navigation</span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Spotaplace</a>
              </div>
              <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                  <li><a href="/members">Home</a></li>
                  <li><a href="/store?v=1">Your Store</a></li>
                  <li class="active"><a href="/store-front?<%= store._id%>">Store Front</a></li>
                  <li><a href="/settings">Settings</a></li>
                  <li><a href="/contact">Contact</a></li>
                  <li><a href="/about">About</a></li>
                  <li><a href="/logout">logout</a></li>
                </ul>
              </div>
            </div>
          </div>

        </div>
      </div>

      <div class="container">
        <div class="row">

        <div class="col-md-5">
          <p>Name: <%= store.name %></p>
          <p>Address: <%= store.addressStreet %></p>
          <p>City: <%= store.addressCity %></p>
          <p>State: <%= store.addressState %></p>
          <p>Province : <%= store.addressProvince %></p>
          <p>Store: <%= store.addressZip %></p>
          <p>Website:  <a href="http://<%= store.website %>"><%= store.website %></a></p>


        </div>
      </div>
      <div style="background-color:yellow;"><%= store.name %></div>
      <div class="row">
        <% for(var i=0; i < hello.length; i++){%>
        <div class="col-md-3">
          <div class="productPlacement"><%= hello[i].name %><img src="<%= hello[i].imagePath %>">
            <p><%= hello[i].description %><p><p>price : <%= hello[i].price%></p>
            <form name="removeProduct" >
              <input type="hidden" value="<%= hello[i]._id %>">
              <button type="submit" class="btn btn-primary remove">remove</button>
            </form>
          </div>
        </div>
        <%} %>
      </div>
      <div class="row">
        <%if(store.reviews){%>
        <% for(var i=0; i < store.reviews.length; i++){%>
          
        <%} %>
        <%} %>
      </div>
          <%if(win){%>
          <h1>Thanks for choosing this vendor when your transacion has been concluded you may choose to give a rating</h1>
              
          <% } %>
          



      <footer>
        <p>&copy; Company 2013</p>
      </footer>
    </div> <!-- /container -->


